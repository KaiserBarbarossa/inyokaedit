PROJECT="${1}"

if [ "${PROJECT}" == "" ]
  then
    echo "USAGE: ./`basename $0` [STYLES FOLDER]"
    exit
fi

SCRIPTDIR=$( cd "$( dirname "$0" )" && pwd )

if [ ! -d ${PROJECT} ]
  then
    echo "Creating styles main folder..."
    mkdir -p ${PROJECT}
fi
cd ${PROJECT}

echo "Calling IWL script..."
bash ${SCRIPTDIR}/GetIWLs ${PROJECT}

cd ${PROJECT}
if [ ! -d img ]
  then
    echo "Creating folder \"img\"..."
    mkdir img
fi
cd img

echo "Downloading images..."
wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/Icons/Portal/anchor.png -O anchor.png
wget -nv https://static-cdn.ubuntu-de.org/img/icon-sprite.png -O icon-sprite.png

if [ ! -d wiki ]
  then
    echo "Creating folder \"wiki\"..."
    mkdir wiki
fi
cd wiki

wget -nv https://static-cdn.ubuntu-de.org/img/wiki/toc.png -O toc.png
wget -nv https://media-cdn.ubuntu-de.org/wiki/attachments/42/17/button.png -O button.png
wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/Vorlagen/Tasten/mouse_left.png -O mouse_left.png
wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/Vorlagen/Tasten/mouse_midd.png -O mouse_midd.png
wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/Vorlagen/Tasten/mouse_right.png -O mouse_right.png
wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/Vorlagen/Tasten/menu.png -O menu.png
wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/Vorlagen/Tasten/power.png -O power.png
wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/Vorlagen/Tasten/tux.png -O tux.png
wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/Vorlagen/Tasten/ubuntu.png -O ubuntu.png
wget -nv https://static-cdn.ubuntu-de.org/img/wiki/heading.png -O heading.png
cd ..


if [ -d flags ]
  then
    echo "Removing obsolete folder \"flags\"..."
    rm -r flags/
fi
cd ..


if [ ! -d Wiki ]
  then
    echo "Creating folder \"Wiki\"..."
    mkdir Wiki
fi
cd Wiki

if [ ! -d FlaggenMap ]
  then
    echo "Creating folder \"Wiki\FlaggenMap\"..."
    mkdir FlaggenMap
fi
cd FlaggenMap

wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/FlaggenMap/ar.png -O ar.png
wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/FlaggenMap/at.png -O at.png
