wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/Vorlagen/StatusIcon/unbekannt.png -O unbekannt.png
wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/Vorlagen/StatusIcon/manuell.png -O manuell.png

cd ..
if [ ! -d PPA ]
  then
    echo "Creating folder \"Wiki\Vorlagen\PPA\"..."
    mkdir PPA
fi
cd PPA
wget -nv https://wiki.ubuntuusers.de/_image?target=Wiki/Vorlagen/PPA/ppa.png -O ppa.png

cd ${PROJECT}

echo "DOWNLOAD FINISHED"
sleep 2
