<?xml version="1.0"?>
<!--
  This document is used to generate community specific menus/toolbars/dropdowns.
  "Path" is relative to share / user data path.
  "Icon" and .tpl/.macro "insert" is relative to "path".
  
  Don't forget to mask the following characters, if used inside a string:
   * & -> &amp;
   * < -> &lt;
   * > -> &gt;
   * ' -> &apos;
   * " -> &quot;
-->
<menu name="Text format" path="">
  <group name="">
    <element name="Folder" insert="&apos;&apos;&apos;%%Folder%%&apos;&apos;&apos;" icon="" />
    <element name="Menu" insert="&apos;&apos;&quot;%%Menu -> Sub menu -> Command%%&quot;&apos;&apos;" icon="" />
    <element name="File" insert="&apos;&apos;&apos;%%File%%&apos;&apos;&apos;" icon="" />
    <element name="Command" insert="`%%Command%%`" icon="" />
  </group>
</menu>
